q:5
    import java.util.Scanner;
    public class divbythreefive{
        public static void main(String[] args) {
            Scanner sc = new Scanner(System.in);
            int a = sc.nextInt();
            if(a%3==0 && a%5==0){
                System.out.println("Yes");
            }
            else{
                System.out.print("No");
            }
            
            sc.close();
        }
        
    }

